			<source ref="id3"/>
			<target ref="id6"/>
			<label kind="guard" x="-569" y="-306">y &gt;= 5</label>
			<label kind="assignment" x="-569" y="-289">y := 0</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id3"/>
			<label kind="guard" x="-612" y="-212">y &gt;= 1</label>
			<label kind="assignment" x="-612" y="-195">y := 0</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="guard" x="-620" y="-85">y &gt;= 30</label>
			<label kind="synchronisation" x="-620" y="-68">arrived?</label>
			<label kind="assignment" x="-620" y="-51">y := 0</label>
		</transition>
	</template>
	<template>
